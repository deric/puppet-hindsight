filename = "<%= @filename %>"
<% @config.sort.each do |k, v| -%>
<% if v.respond_to?(:each) -%>
<%= k %> = {
<% v.sort.each do |hk, hv| -%>
  <%= hk %> = <%= scope.call_function('hindsight_escape', [hv]) %>,
<% end -%>
}
<% else -%>
<%= k %> = <%= scope.call_function('hindsight_escape', [v]) %>
<% end -%>
<% end -%>