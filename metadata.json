{
  "name": "deric-hindsight",
  "version": "0.3.0-dev",
  "author": "deric",
  "summary": "Manage Hindsight log processing service",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-hindsight",
  "project_page": "https://github.com/deric/puppet-hindsight",
  "issues_url": "https://github.com/deric/puppet-hindsight/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.2.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 1.2.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
